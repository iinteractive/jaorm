{
  "name": "jaorm",
  "description": "Just Another ORM",
  "version": "0.0.1",
  "main": "lib/schema.js",
  "author": {
    "name": "Andrew Nelson",
    "email": "andrew.nelson@iinteractive.com"
  },
  "dependencies": {
    "knex": "^0.19.4",
    "pluralize": "^8.0.0"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "chalk": "2.4.2",
    "eslint": "5.6.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "3.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-react": "7.11.1",
    "husky": "1.3.1",
    "lint-staged": "8.1.5",
    "mocha": "6.1.2",
    "mysql": "^2.17.1",
    "mysql2": "^2.1.0",
    "nyc": "13.3.0",
    "pg": "7.12.1",
    "prettier": "1.16.4",
    "sqlite3": "4.1.0"
  },
  "engines": {
    "node": "12.11.1",
    "npm": "6.11.3"
  },
  "scripts": {
    "tests": "node run_tests.js",
    "coverage": "nyc npm run tests"
  },
  "nyc": {
    "check-coverage": true,
    "exclude": [
      "run_tests.js",
      "test/**"
    ],
    "lines": 99,
    "statements": 99,
    "functions": 99,
    "branches": 99,
    "reporter": [
      "text",
      "text-summary"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.js": [
        "prettier --list-different"
      ]
    }
  }
}